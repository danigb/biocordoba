- title 'Editar sector'

#sectors.edit

  - form_for(@sector) do |f|
    %p
      = f.label :name, 'Nombre'
      = f.text_field :name
      = f.error_message_on :name
    %p
      = f.label :name, 'English Name'
      = f.text_field :english_name
      = f.error_message_on :english_name
    %p.enviar  
      = f.submit 'Actualizar'
    = link_to 'Ver', @sector
    |
    = link_to 'Volver', sectors_path
